{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "ConnectionStrings": {
    "ElearningIskoop": "Host=localhost;Port=5432;Database=ElearningIskoop;Username=postgres;Password=1234;"
  },
  "OutboxProcessor": {
    "ProcessingIntervalSeconds": 5,
    "BatchSize": 100,
    "KeepProcessedEventsDays": 30,
    "MaxRetryAttempts": 3,
    "RetryBaseDelayMinutes": 2,
    "EnableAutoCleanup": true,
    "CleanupIntervalHours": 24,
    "EnableProcessing": true,
    "PriorityProcessing": true,
    "MaxProcessingTimeMinutes": 5
  },
  "AllowedHosts": "*",
  "ELearningIskoop": {
    "Jwt": {
      "SecretKey": "development-secret-key-minimum-32-characters-long",
      "Issuer": "ElearningIssuer",
      "Audience": "ElearningAudience",
      "ExpiryInMinutes": 60
    }
  }

}